general {
  colors = true
  interval = 1
  separator = "|"
}

order += "wireless _first_"
order += "ethernet _first_"
order += "tztime local"

wireless _first_ {
  format_up = "WLS"
  format_down = "WLS"
}

ethernet _first_ {
  format_up = "ETH"
  format_down = "ETH"
}

tztime local {
  format = "%I:%M / %h %d, %Y "
}